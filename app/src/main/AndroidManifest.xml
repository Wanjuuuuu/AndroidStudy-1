<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.osori.androidstudy">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".week1.CustomDialogActivity" />
        <activity android:name=".week2.ViewPagerActivity" />
        <activity android:name=".week3.CatActivity" />
        <activity android:name=".week4.ConstraintLayoutActivity"/>
        <activity android:name=".week5.FloatingViewServiceActivity"/>

        <service android:name=".week5.FloatingViewService"
            android:enabled="true"
            android:exported="false"/>
    </application>

</manifest>